
sig AID, BID {}
one sig b1, b2 extends BID {}
one sig p0_AID, a1 extends AID {}

state Root {
	conc A [AID] {
		conc B [BID] {
			state S1 {
				trans t2 {}
				trans t3 {
					from Root/A/B/S1[p0_AID, b1]
					goto Root/A/B/S1[p0_AID, b2]
				}
			}
		}
		default state S2 {
			trans t1 {
				goto Root/A/B/S1[a1,b1]
			}
		}
	}
}