sig AID, BID {}
one sig b1,b3,b4,b5,b6 extends BID {}
one sig a2, a1,a3,a4,a5,a6 extends AID {}

state Root {
	conc A [AID] {
		conc B [BID] {
			state S1 {
				trans t1 {
					from Root/A/B/S1[a1, b1]
					goto Root/A[a2]
				}
			}
		}
	}
	trans t2 {
		from Root/A/B/S1[a3,b3]
		goto Root/A/B/S1[a4,b4]
	}
	trans t3 {
		from Root/A/B/S1[a5,b5]
		goto Root/A/B[a6,b6]
	}
}